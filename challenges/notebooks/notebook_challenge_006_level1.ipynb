        "%%sql\n",
        "select LastName\n",
        "      , strftime('%Y', BirthDate) as ano_de_nascimento\n",
        "      , strftime('%Y', 'now') - strftime('%Y', BirthDate) as idade\n",
        "  from Employee\n",
        "order by idade desc\n",
        "\n",
